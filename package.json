    "postcss-preset-env": "^7.7.2",
    "postcss-safe-parser": "^6.0.0",
    "prettier": "^2.7.1",
    "svelte": "^3.49.0",
    "svelte-check": "^2.2.7",
    "svelte-preprocess": "^4.9.8",
    "terser": "^5.14.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "^3.0.2"
  },
  "dependencies": {
    "smoothscroll-polyfill": "^0.4.4",
    "svelte-spa-router": "^3.2.0"
  },
  "browserslist": "> 0.25%, iOS 9"
